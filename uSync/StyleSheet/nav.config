<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Stylesheet>
  <Name>nav</Name>
  <FileName>nav</FileName>
  <Content><![CDATA[nav a {
    -webkit-transition: background-color .2s;
    -moz-transition: background-color .2s;
    -ms-transition: background-color .2s;
    -o-transition: background-color .2s;
    transition: background-color .2s;
}

/* Hide some elements as default. Should only be visible on mobile*/
#top-nav .toggle,
#top-nav .search-form,
#top-nav .top-links,
#top-nav .dyn-menu ul {
    display: none;
}

/* == Main navigation == */
#top-nav {
    overflow: hidden;
    background-color: #E44D26;
    will-change: opacity;
}

    #top-nav ul {
        padding: 0;
        list-style-type: none;
    }

    #top-nav .dyn-menu li {
        display: inline-block;
        *display: inline;
    }

    #top-nav a {
        color: white;
        padding: 0 30px;
    }

    #top-nav .dyn-menu .active > ul {
        display: block; /* Only display sub-ul's in 'active' li's '*/
    }
/* == End of Main navigation == */


/* == Left navigation == */
#left-nav ul {
    list-style-type: none;
    padding: 0;
}

#left-nav a {
    background-color: #F16529;
    color: #FFF;
    padding: 9px 12px;
    display: block;
}

#left-nav > ul > li {
    background: #FFECE4; /* Background for submenus */
}

    #left-nav > ul > li.active > a {
        background-color: #D25925; /* Slightly darker active menu */
    }

#left-nav > ul ul {
    padding-left: 5%; /* Move every submenu a bit to the right */
}

#left-nav ul ul a {
    background-color: transparent;
    color: #333;
}

#left-nav ul ul ul a {
    font-size: 13px;
}

#left-nav.dyn-menu ul ul {
    display: none;
}

#left-nav.dyn-menu .active > ul {
    display: block; /* Only display sub-ul's in 'active' li's '*/
}
/* == End of Left navigation == */


/* == Dynamic navigation == */
.dyn-menu a {
    position: relative;
}

.dyn-menu .toggle {
    position: absolute;
    height: 100%;
    width: 45px;
    right: 0;
    top: 11px;
    text-align: center;
}

.dyn-menu li.active > a > .toggle:before, .maximizing > a > .toggle:before {
    content: "\e602"; /* Minus-sign TODO: Content kan beh�va �ndras */
}

.minimizing > a > .toggle:before {
    content: "\e601" !important; /* Plus-sign  TODO: Content kan beh�va �ndras */
}
/* == End of Dynamic navigation == */



/* == Top links == */
.top-links li {
    margin-left: 6px;
    display: inline-block;
}

.top-links span, .top-links img {
    vertical-align: middle;
}

.top-links img {
    max-width: 15px;
}

/* Toplinks in the mobile menu */
nav .top-links {
    border: 1px solid #3C3C3C;
    text-align: center;
}
/* == End of Top links == */


/* == Mobile menu stuff == */
.mobile-menu-toggle {
    border: 1px solid #333;
    padding: 5px 8px;
    display: none;
    border-radius: 3px;
    float: right;
}

    .mobile-menu-toggle i {
        vertical-align: middle;
        margin-right: 5px;
        font-size: 21px;
    }


.mobile-menu-open #top-nav {
    display: block;
}

    /* Change the toggle-icon to a cross */
    .mobile-menu-open .mobile-menu-toggle .icon-menu:before, .mobile-menu-enter .mobile-menu-toggle .icon-menu:before {
        content: "\e604"; /* TODO denna kan du beh�va �ndra till content f�r ditt kryss. Det ska bli kryss n�r mobilmenyn �r �ppen/�ppnas */
    }

.mobile-menu-leave .mobile-menu-toggle .icon-menu:before {
    content: "\e600"; /* TODO denna kan du beh�va �ndra till content f�r din hamburgare. Hamburgaren ska komma tillbaka innan animering �r klar. Det k�nns snabbare d� */
}
/* == End of Mobile menu stuff == */


/* Pulsera plus-knappen n�r det laddar f�r att ge anv�ndaren n�gon respons om det skulle ta l�ng tid */
.dyn-menu .loading .icon-plus {
    -webkit-animation: 2s opaque_leave infinite;
    -moz-animation: 2s opaque_leave infinite;
    -o-animation: 2s opaque_leave;
    animation: 2s opaque_leave infinite;
}
]]></Content>
</Stylesheet>