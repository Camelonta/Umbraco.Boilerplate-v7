<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Stylesheet>
  <Name>layout</Name>
  <FileName>layout</FileName>
  <Content><![CDATA[/* == Layout rules divide the page into sections. Layouts hold one or more modules together. == */

.container {
    max-width: 100%;
    width: 1080px;
    margin: 0 auto;
    padding-right: 15px;
    padding-left: 15px;
}


/* == Header == */
header {
    background: #f16529;
}
/* == End of Header == */

.umb-grid .container {
    padding: 0; /* This is a test. */
}


/* == Left sidebar == */
#l-sidebar {
    width: 22%;
    margin-right: 3%;
    float: left;
}
/* == End of Left sidebar == */


/* == Footer == */
footer {
    background: #f16529;
    border-top: 20px solid #e44d26;
    color: white;
    padding: 20px 0;
    margin-top: 70px;
}
/* == End of Footer == */


/* == Main content == */
main {
    float: left;
    width: 53%;
    min-height: 53vh;
}

.no-leftnav main {
    width: 77%;
}

/* Pages without right column */
.pagetype-commonpagegrid main,
.pagetype-newslist main,
.pagetype-news main {
    width: 75%;
}
.pagetype-commonpagegrid.no-leftnav main{
    width: 100%;
}

aside {
    margin-top: 20px;
    float: right;
    width: 20%;
    border-top: 20px solid #e44d26;
    color: white;
    padding: 0 15px 10px;
    background: #f16529;
}
/* == End of Main content == */
]]></Content>
</Stylesheet>